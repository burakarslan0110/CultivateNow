@using EntityLayer.Concrete
@model List<Adress>
<div class="w3l_header_left">
    @foreach(var item in Model)
    {
        <ul>
            <li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>@item.Description1 @item.Description3</li>
            <li><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span>@item.Phone</li>
            <li><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> <a href="mailto:@item.Description2">@item.Description2</a></li>
        </ul>
    }
</div>