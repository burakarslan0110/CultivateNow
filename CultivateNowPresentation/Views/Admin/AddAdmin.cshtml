@model EntityLayer.Concrete.Admin
@{
    ViewData["Title"] = "AddAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Yeni Admin Kaydı</h4>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label>Kullanıcı Adı:</label>
                    <input type="text" asp-for="Username" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Şifre:</label>
                    <input type="password" rows="5" asp-for="Password" class="form-control"></input>
                </div>
                <button type="submit" class="btn btn-outline-info">Kaydet</button>
            </form>
        </div>
    </div>
</div>

